@import "../../static/fonts/iconfont.wxss";
.myzhuanxian{
  width: 100%;
}
.myzhuanxian .myzhuanxian_li{
  position: relative;
  width: 100%;
  height: 170rpx;
  border-bottom: 2rpx solid #eee;
}
.myzhuanxian .myzhuanxian_li .zhuanxian{
  position: absolute;
  top: 30rpx;
  left: 30rpx;
  font-size: 36rpx;
  color: #333;
}
.myzhuanxian .myzhuanxian_li .time{
  position: absolute;
  top: 100rpx;
  left: 30rpx;
  font-size: 30rpx;
  color: #666;
}
.myzhuanxian .myzhuanxian_li .edit,
.myzhuanxian .myzhuanxian_li .del{
  position: absolute;
  top: 70rpx;
  right: 150rpx;
  font-size: 30rpx;
  color: #333;
}
.myzhuanxian .myzhuanxian_li .del{
  right: 30rpx;
}

button{
  width: 690rpx;
  margin: 50rpx 0 50rpx 30rpx;
  background-color: #ff5b5a;
  color: #fff;
}
.button-hover{
  background-color: #ff7271;
  color: #fff;
}
button text{
  padding: 0 10rpx;
}