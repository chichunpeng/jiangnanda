.title{
  width: 550rpx;
  margin-left: 100rpx;
  height: 120rpx;
  font-size: 48rpx;
  line-height: 120rpx;
  color: #ffc600;
  text-align: center;
  border-bottom: 10rpx solid #ddff00;
}
.li{
  width: 690rpx;
  margin-left: 30rpx;
  margin-top: 20rpx;
}
.li .con{
  display: inline-block;
  width: 95%;
  line-height: 40rpx;
  font-size: 30rpx;
  color: #333;
  vertical-align: top;
}
.li .num{
  display: inline-block;
  width: 5%;
  line-height: 40rpx;
  font-size: 30rpx;
  color: #333;
  vertical-align: top;
}