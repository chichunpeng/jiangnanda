.search{
  position: relative;
  width: 100%;
  height: 105rpx;
  border-bottom: 2rpx solid #eee;
}
.search .start,
.search .end{
  position: absolute;
  top: 20rpx;
  left: 20rpx;
  width: 200rpx;
  height: 60rpx;
  border: 2rpx solid #bbb;
  font-size: 30rpx;
  line-height: 60rpx;
  color: #333;
  text-align: center;
  border-radius: 8rpx;
  overflow: hidden;
}
.search .end{
  left: 330rpx;
}
.search .start .iconfont,
.search .end .iconfont{
  position: absolute;
  top: 5rpx;
  right: 8rpx;
  font-size: 20rpx;
  color: #666;
}
.search .Btn{
  position: absolute;
  top: 20rpx;
  right: 30rpx;
  width: 155rpx;
  height: 64rpx;
  font-size: 30rpx;
  line-height: 64rpx;
  background-color: #ff5b5a;
  color: #fff;
  text-align: center;
  border-radius: 8rpx;
}
.search .line{
  position: absolute;
  top: 50rpx;
  left: 248rpx;
  width: 60rpx;
  height: 3rpx;
  background: #333;
}
.changeCity,
.changePrice{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: #fff;
  z-index: 100;
  overflow: scroll;
}
.changeCity .local{
  width: 100%;
  height: 160rpx;
}
.changeCity .allcity{
  width: 100%;
}
.changeCity .local .tit,
.changeCity .allcity .tit,
.changeCity .local .city,
.changeCity .allcity .city,
.changePrice .tit,
.changePrice .allprice{
  position: relative;
  width: 96%;
  padding-left: 4%;
  height: 80rpx;
  background-color: #eee;
  font-size: 30rpx;
  color: #999;
  line-height: 80rpx;
}
.changeCity .local .city,
.changeCity .allcity .city,
.changePrice .allprice{
  background-color: #fff;
  color: #333;
}
.changeCity .allcity .city,
.changePrice .allprice{
  border-bottom: 2rpx solid #eee;
}
.close,.back{
  position: absolute;
  top: 10rpx;
  right: 30rpx;
  width: 90rpx;
  height: 60rpx;
  font-size: 30rpx;
  color: #fff;
  line-height: 60rpx;
  text-align: center;
  background-color: #ff5b5a;
  border-radius: 8rpx;
} 